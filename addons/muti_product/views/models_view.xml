	<!-- 		Product Category -->
		<record model="ir.ui.view" id="product_category_form">
            <field name="name">Product Category</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <label for="name" position = "before">
                	<label for = "group_code" string = "Group Code"/>
	               	<field name = "group_code"/>
	               	<label for = "barcode_affix" string = "Barcode Affix"/>
	               	<field name = "barcode_affix" required = "True"/>
	            </label>
	            
	            <group name = "first" position="after">
	            	<group string = "Products">
	            		<field name = "group_ids">
	            		
	            		<tree string ="Product" default_order = "barcode">
	            			<field name ="barcode"/>
	            			<field name="name"/>
	            		</tree>
	            	</field>
	            </group>
	            </group>

            </field>
        </record>
       

